    "babel-preset-stage-0": "^6.24.1",
    "babel-runtime": "^6.11.6",
    "chai": "^3.5.0",
    "cross-env": "^2.0.1",
    "css-loader": "^0.28.10",
    "enzyme": "^2.4.1",
    "eslint": "^4.18.2",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-flowtype": "^2.46.1",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-plugin-react": "^7.7.0",
    "flow-bin": "^0.66.0",
