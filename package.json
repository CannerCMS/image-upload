    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^1.8.0",
    "mocha": "^2.4.5",
    "precommit-hook-eslint": "^3.0.0",
    "prettier": "^1.10.2",
    "react": "^16.2.0",
    "react-addons-test-utils": "^15.3.1",
    "react-dom": "^16.2.0",
